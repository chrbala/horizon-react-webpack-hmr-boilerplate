{
  "name": "horizon-react-webpack-hmr-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for horizon-react and webpack, with hot module reloading",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrbala/horizon-react-webpack-hmr-boilerplate"
  },
  "keywords": [
    "horizon",
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Chris Bala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrbala/horizon-react-webpack-hmr-boilerplate/issues"
  },
  "homepage": "https://github.com/chrbala/horizon-react-webpack-hmr-boilerplate",
  "dependencies": {
    "@horizon/client": "1.1.3",
    "@horizon/server": "1.1.3",
    "babel-core": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "cross-env": "^1.0.7",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "0.7.1",
    "express": "^4.13.3",
    "horizon-react": "0.3.0",
    "linter": "git+https://github.com/chrbala/linter.git",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "redux": "3.5.2",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}